{
  "model": "SeisMoE",
  "data": "ETHZ",
  "trainer_args": {
    "accelerator": "gpu",
    "devices": 1,
    "max_epochs": 50
  },
  "model_args": {
    "base_model": "stead",
    "num_experts": 8,
    "num_experts_per_token": 2,
    "moe_loss_coef": 0.02,
    "lr": 1e-5,
    "moe_lr": 1e-4,
    "warmup_steps": 500,
    "rotate_array": true,
    "freeze_non_moe": true
  }
}